APP_NAME="Snipe-IT"
APP_ENV=production
APP_KEY=base64:YOUR_APP_KEY_HERE
APP_DEBUG=false
APP_URL=http://localhost

LOG_CHANNEL=stack
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=snipeit
DB_USERNAME=root
DB_PASSWORD=

BROADCAST_DRIVER=log
CACHE_DRIVER=file
FILESYSTEM_DISK=local
QUEUE_CONNECTION=sync
SESSION_DRIVER=file
SESSION_LIFETIME=120

MEMCACHED_HOST=127.0.0.1

REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=smtp
MAIL_HOST=mailhog
MAIL_PORT=1025
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS="admin@yourcompany.com"
MAIL_FROM_NAME="Snipe-IT"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

PUSHER_APP_ID=
PUSHER_APP_KEY=
PUSHER_APP_SECRET=
PUSHER_HOST=
PUSHER_PORT=443
PUSHER_SCHEME=https
PUSHER_APP_CLUSTER=mt1

VITE_PUSHER_APP_KEY="${PUSHER_APP_KEY}"
VITE_PUSHER_HOST="${PUSHER_HOST}"
VITE_PUSHER_PORT="${PUSHER_PORT}"
VITE_PUSHER_SCHEME="${PUSHER_SCHEME}"
VITE_PUSHER_APP_CLUSTER="${PUSHER_APP_CLUSTER}"

# LDAPS Configuration for Active Directory
LDAP_ENABLED=true
LDAP_SERVER=ldaps://YOUR_SERVER_IP
LDAP_PORT=636
LDAP_TLS=false
LDAP_SERVER_CERT_IGNORE=true
LDAP_BASEDN=DC=YOUR_DOMAIN,DC=LOCAL
LDAP_USERNAME_FIELD=samaccountname
LDAP_AUTH_FILTER_QUERY=samaccountname=
LDAP_VERSION=3
LDAP_IS_AD=true
LDAP_AD_DOMAIN=YOUR_DOMAIN.LOCAL
LDAP_UNAME=Admin@YOUR_DOMAIN.LOCAL
LDAP_PWORD=YOUR_ADMIN_PASSWORD

# LDAP Field Mappings
LDAP_FNAME_FIELD=givenName
LDAP_LNAME_FIELD=sn
LDAP_EMAIL=mail
LDAP_PHONE_FIELD=telephoneNumber
LDAP_JOBTITLE=title
LDAP_DEPT=department
LDAP_MANAGER=manager
LDAP_LOCATION=physicalDeliveryOfficeName
LDAP_COUNTRY=c
LDAP_EMP_NUM=employeeNumber
LDAP_FILTER=&(cn=*)
LDAP_PW_SYNC=true

# LDAPS SSL/TLS Settings
LDAPTLS_REQCERT=never
LDAP_TLS_REQCERT=never
LDAPTLS_CACERT=
LDAP_TLS_CACERT=

# Additional LDAP Settings
LDAP_TIMEOUT=30
LDAP_NETWORK_TIMEOUT=30
LDAP_REFERRALS=0
